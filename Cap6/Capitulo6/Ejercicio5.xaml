<Window x:Class="Cap6.Capitulo6.Ejercicio5"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cap6.Capitulo6"
        mc:Ignorable="d"
        Title="Ejercicio 5" Height="480" Width="500">
    <Grid  >
        <Label Content="Calificacion" />
        <TextBox x:Name="calificacion"  />
        <Button  x:Name="agregar" Content="Agregar" Click="agregar_Click" />
        <Button  x:Name="Imprimir" Content="Imprimir"  Click="Imprimir_Click"/>
        <ListBox  x:Name="pantalla"  d:ItemsSource="{d:SampleData ItemCount=5}"/>
    </Grid>     
</Window>